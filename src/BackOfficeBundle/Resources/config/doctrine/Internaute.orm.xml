<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="BackOfficeBundle\Entity\Internaute" repository-class="BackOfficeBundle\Repository\internauteRepository" table="internaute">
    <indexes>
      <index name="voiture_id" columns="voiture_id"/>
      <index name="ville_id" columns="ville_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenom" type="string" column="prenom" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tel" type="string" column="tel" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mail" type="string" column="mail" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="voiture" target-entity="Voiture" fetch="LAZY">
      <join-columns>
        <join-column name="voiture_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="ville" target-entity="Ville" fetch="LAZY">
      <join-columns>
        <join-column name="ville_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
